{# mustache list template #}
{% verbatim %}
<script id="asm-translations-tbl-tpl" type="x-tmpl-mustache">
{{#translations}}
                <tr>
                    <td>{{ transKey }}</td>
                    <td>{{ translation }}</td>
                    <td>{{ transLocale }}</td>
                    <td>{{ messageDomain }}</td>
                    <td>{{ dateCreated }}</td>
                    <td>{{ dateUpdated }}</td>
                    <td>
                        <div class="btn-group btn-group-xs">
                            <button title="{% endverbatim %}{{ 'Edit'|trans }}{% verbatim %}" type="button" class="btn btn-default btn-xs asm-edit-btn" data-link="{% endverbatim %}{{ path('asm_translation_loader.admin.form') }}{% verbatim %}" aria-label="{% endverbatim %}{{ 'edit'|trans }}{% verbatim %}" data-key="{{ transKey }}" data-locale="{{ transLocale }}" data-domain="{{ messageDomain }}">
                                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                            </button>
                            <button title="{% endverbatim %}{{ 'Delete'|trans }}{% verbatim %}" type="button" class="btn btn-default btn-xs asm-delete-btn"  data-link="{% endverbatim %}{{ path('asm_translation_loader.admin.delete') }}{% verbatim %}" aria-label="{% endverbatim %}{{ 'delete'|trans }}{% verbatim %}" data-key="{{ transKey }}" data-locale="{{ transLocale }}" data-domain="{{ messageDomain }}">
                                <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                            </button>
                        </div>
                    </td>
                </tr>
{{/translations}}
</script>
{% endverbatim %}
