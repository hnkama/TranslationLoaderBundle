{# mustache list template #}
{% verbatim %}
<script id="asm-translations-tbl-tpl" type="x-tmpl-mustache">
                <tr>
                    <td>{{ translation.transKey }}</td>
                    <td>{{ translation.translation }}</td>
                    <td>{{ translation.transLocale }}</td>
                    <td>{{ translation.messageDomain }}</td>
                    <td>{{ translation.dateCreated|date('Y-m-d') }}</td>
                    <td>{{ translation.dateUpdated|date('Y-m-d') }}</td>
                    <td>
                        <div class="btn-group btn-group-xs">
                            <button title="{% endverbatim %}{{ 'Edit'|trans }}{% verbatim %}" type="button" class="btn btn-default btn-xs asm-edit-btn" data-link="{% endverbatim %}{{ path('asm_translation_loader.admin.form') }}{% verbatim %}" aria-label="{% endverbatim %}{{ 'edit'|trans }}{% verbatim %}" data-key="{{ translation.transKey }}" data-locale="{{ translation.transLocale }}" data-domain="{{ translation.messageDomain }}">
                                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                            </button>
                            <button title="{% endverbatim %}{{ 'Delete'|trans }}{% verbatim %}" type="button" class="btn btn-default btn-xs asm-delete-btn"  data-link="{% endverbatim %}{{ path('asm_translation_loader.admin.delete') }}{% verbatim %}" aria-label="{% endverbatim %}{{ 'delete'|trans }}{% verbatim %}" data-key="{{ translation.transKey }}" data-locale="{{ translation.transLocale }}" data-domain="{{ translation.messageDomain }}">
                                <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                            </button>
                        </div>
                    </td>
                </tr>
</script>
{% endverbatim %}
